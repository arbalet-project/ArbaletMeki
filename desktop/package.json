{
  "name": "arbalet_meki_live",
  "version": "0.0.1",
  "description": "Desktop Application for Arbalet Meki Live",
  "main": "main.js",
  "homepage": "http://www.arbalet-project.org/",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "Arbalet",
    "Meki",
    "Live",
    "Education",
    "Pixel",
    "Art"
  ],
  "author": "Lucas Demarais <lucdems@gmail.com>",
  "devDependencies": {
    "electron": "^4.0.3",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {
    "electron-reload": "^1.4.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "ip": "^1.1.5",
    "ip6addr": "^0.2.2",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "path": "^0.12.7",
    "rgb-hex-converter": "^0.2.1",
    "socket.io": "^2.2.0"
  },
  "build":{
    "appId": "com.example.arbalet",
    "productName": "Arbalet Meki Live",
    "mac":{
      "category": "education"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  }
}
